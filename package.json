{
  "name": "react-redux-ts-starter-kit",
  "version": "1.0.0",
  "description": "starter kit which based on rollup/es6/typescript/react/redux/scss",
  "repository": "git+https://github.com/AndreyUtka/react-redux-ts-starter-kit.git",
  "author": "AndreyUtka <AndreyUtka@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AndreyUtka/react-redux-ts-starter-kit/issues"
  },
  "homepage": "https://github.com/AndreyUtka/react-redux-ts-starter-kit#readme",
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist/build/",
    "prebuild": "npm run clean && npm run notify && npm run prebuild:static",
    "prebuild:static": "node build-process/vendor.js && npm run build:html",
    "start": "BUILD_TYPE='start' npm run dev:start",
    "build-dev": "BUILD_TYPE='dev' npm run build:dev",
    "build-prod": "BUILD_TYPE='prod' npm run build:prod",
    "build:dev": "npm run prebuild && npm run build:ts:dev && npm run build:scss",
    "build:prod": "npm run prebuild && npm run build:ts:prod && npm run build:scss",
    "postbuild:dev": "npm run rollup:dev && npm run styleguide",
    "postbuild:prod": "npm run rollup:prod && npm run styleguide",
    "build:ts:dev": "tsc -p config/tsconfig.dev.json",
    "build:ts:prod": "tsc -p config/tsconfig.prod.json",
    "build:scss": "node build-process/styles/build.js",
    "build:html": "node build-process/template.js",
    "dev:start": "run-s prebuild build:ts:dev build:scss dev:watch",
    "dev:watch": "run-p dev:ts dev:scss dev:rollup server:run",
    "dev:ts": "npm run build:ts:dev -- --watch",
    "dev:scss": "node build-process/styles/watch.js",
    "dev:rollup": "npm run rollup:dev -- --watch",
    "rollup:dev": "rollup -c build-process/rollup/rollup.config.dev.js",
    "rollup:prod": "node build-process/rollup/rollup.config.prod.js",
    "notify": "node build-process/helpers/notify.js",
    "styleguide": "npm run lint:ts && npm run lint:scss && npm run format:check",
    "lint:all:watch": "run-p lint:scss:watch lint:ts:watch",
    "lint:ts:watch": "onchange 'src/**/*.{ts,tsx}' -d 5000 -- npm run lint:ts",
    "lint:ts": "tslint --project ./tsconfig.json --force --format stylish",
    "lint:scss:watch": "onchange 'src/**/*.scss' -d 5000 -- npm run lint:scss",
    "lint:scss": "stylelint 'src/**/*.scss' || exit 0",
    "format:fix": "prettier -l --write '{build-process,config,src}/**/*.{ts,tsx,json,js,scss}' || exit 0",
    "format:check": "prettier -l '{build-process,config,src}/**/*.{ts,tsx,json,js,scss}'",
    "server:run": "node build-process/helpers/browser.js & nodemon --watch server server/app.js",
    "test": "NODE_ENV=test jest --config config/jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "dependencies": {
    "@types/react": "16.0.34",
    "@types/react-dom": "16.0.3",
    "@types/react-router-dom": "4.2.3",
    "chalk": "2.3.0",
    "eyeglass": "1.3.0",
    "google-closure-compiler-js": "20180101.0.0",
    "handlebars": "4.0.11",
    "immutable": "3.8.2",
    "node-sass": "4.7.2",
    "prettier": "1.10.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "rollup": "0.54.0",
    "rollup-plugin-node-resolve": "3.0.2",
    "rollup-plugin-replace": "2.0.0",
    "stylelint": "8.4.0",
    "tslib": "1.8.1",
    "tslint": "5.9.1",
    "tslint-react": "3.4.0",
    "typescript": "2.6.2"
  },
  "devDependencies": {
    "@types/jest": "22.0.1",
    "browser-sync": "2.23.5",
    "chokidar": "2.0.0",
    "express": "4.16.2",
    "jest": "22.0.6",
    "morgan": "1.9.0",
    "nodemon": "1.14.11",
    "npm-run-all": "4.1.2",
    "onchange": "3.3.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "ts-jest": "22.0.1"
  }
}
