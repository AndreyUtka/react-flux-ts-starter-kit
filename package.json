{
  "name": "react-flux-ts-starter-kit",
  "version": "1.0.0",
  "description": "starter kit which based on rollup/es6/typescript/react/flux/rx/scss",
  "repository": "git+https://github.com/AndreyUtka/react-flux-ts-starter-kit.git",
  "author": "AndreyUtka <AndreyUtka@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AndreyUtka/react-flux-ts-starter-kit/issues"
  },
  "homepage": "https://github.com/AndreyUtka/react-flux-ts-starter-kit#readme",
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "clean": "node scripts/helpers/clean.js",
    "prebuild": "npm run clean && npm run notify && npm run prebuild:static",
    "prebuild:static": "node scripts/vendor.js && npm run build:html",
    "start": "BUILD_TYPE='start' npm run dev:start",
    "build-dev": "BUILD_TYPE='dev' npm run build:dev",
    "build-prod": "BUILD_TYPE='prod' npm run build:prod",
    "build:dev": "npm run prebuild && npm run build:ts:dev && npm run build:scss",
    "build:prod": "npm run prebuild && npm run build:ts:prod && npm run build:scss",
    "postbuild:dev": "npm run rollup:dev && npm run styleguide",
    "postbuild:prod": "npm run rollup:prod && npm run styleguide",
    "build:ts:dev": "tsc -p config/tsconfig.dev.json",
    "build:ts:prod": "tsc -p config/tsconfig.prod.json",
    "build:scss": "node scripts/styles/build.js",
    "build:html": "node scripts/template.js",
    "dev:start": "run-s prebuild build:ts:dev build:scss dev:watch",
    "dev:watch": "run-p dev:ts dev:scss dev:rollup server:run",
    "dev:ts": "npm run build:ts:dev -- --watch",
    "dev:scss": "node scripts/styles/watch.js",
    "dev:rollup": "npm run rollup:dev -- --watch",
    "rollup:dev": "rollup -c scripts/rollup/rollup.config.dev.js",
    "rollup:prod": "node scripts/rollup/rollup.config.prod.js",
    "notify": "node scripts/helpers/notify.js",
    "styleguide": "npm run lint:ts && npm run lint:scss && npm run format:check",
    "lint:all:watch": "run-p lint:scss:watch lint:ts:watch",
    "lint:ts:watch": "onchange 'src/**/*.{ts,tsx}' -d 5000 -- npm run lint:ts",
    "lint:ts": "tslint --project ./tsconfig.json --force --format stylish",
    "lint:scss:watch": "onchange 'src/**/*.scss' -d 5000 -- npm run lint:scss",
    "lint:scss": "stylelint 'src/**/*.scss' || exit 0",
    "format:fix": "prettier -l --write '{scripts,config,src}/**/*.{ts,tsx,json,js,scss}' || exit 0",
    "format:check": "prettier -l '{scripts,config,src}/**/*.{ts,tsx,json,js,scss}'",
    "server:run": "node scripts/helpers/browser.js & nodemon --watch server server/app.js",
    "test": "NODE_ENV=test jest --config config/jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "dependencies": {
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/react-router-dom": "4.2.4",
    "chalk": "2.3.2",
    "csso": "3.5.0",
    "eyeglass": "1.3.0",
    "google-closure-compiler-js": "20180204.0.0",
    "handlebars": "4.0.11",
    "immutable": "3.8.2",
    "node-sass": "4.7.2",
    "prettier": "1.11.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2",
    "rollup": "0.56.3",
    "rollup-plugin-node-resolve": "3.0.3",
    "rollup-plugin-replace": "2.0.0",
    "rxjs": "5.5.6",
    "stylelint": "9.1.1",
    "tslib": "1.9.0",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.9",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/jest": "22.1.4",
    "browser-sync": "2.23.6",
    "chokidar": "2.0.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "express": "4.16.2",
    "jest": "22.4.2",
    "morgan": "1.9.0",
    "nodemon": "1.17.1",
    "npm-run-all": "4.1.2",
    "onchange": "3.3.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "ts-jest": "22.4.1"
  }
}
