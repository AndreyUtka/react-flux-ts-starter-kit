{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "no-namespace": false,
    "only-arrow-functions": false,
    "no-var-requires": false,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ]
  },
  "ordered-imports": false,
  "rulesDirectory": []
}
